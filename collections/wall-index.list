<div class="grid-container">
    {.repeated section items}
        {.if customContent.featured}
            {@|apply featured-video.block}
        {.end}
    {.end}
  <ul class="effect-2" id="wallGrid">
    {.repeated section items}<script>console.log({@|json})</script>
      {.if customContent.featured}
      {.or}{.equal? customContent.customType "wall-item-link"}
        <a href="{clickthroughUrl}" data-first-order="{customContent.firstOrder}"></a>
      {.or}
        {@|apply wall-slide-block.block}
      {.end}
      {.end}
    {.end}
  </ul>
</div>