<div class="grid-container">
    {.repeated section items}
        {.if customContent.featured}
            {@|apply featured-video.block}
        {.end}
    {.end}
  <ul class="effect-2" id="grid">
    {.repeated section items}
      {.if customContent.featured}
      {.or}
        {@|apply wall-slide-block.block}
      {.end}
    {.end}
  </ul>
</div>