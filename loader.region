<!DOCTYPE html {.if authenticatedAccount} authenticated {.end}>
<html class="no-js">
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=0"/>
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black"/>
    <link rel="apple-touch-icon" href="{website.logoImageUrl}">
    <link rel="apple-touch-icon" sizes="76x76" href="{website.logoImageUrl}">
    <link rel="apple-touch-icon" sizes="120x120" href="{website.logoImageUrl}">
    <link rel="apple-touch-icon" sizes="152x152" href="{website.logoImageUrl}">
    <link rel="apple-touch-startup-image" href="{website.logoImageUrl}">
    {.if authenticatedAccount}
    {squarespace-headers}
    {.or}
    <title>{.if item}{item.title} — {website.siteTitle}{.or}{.if collection}{collection.title} — {website.siteTitle}{.or}{website.siteTitle}{.end}{.end}</title>
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico"/>
    <link rel="canonical" href="{.if item}{website.baseUrl}{item.fullUrl}{.or}{.if collection}{.if collection.homepage}{website.baseUrl}/{.or}{website.baseUrl}{collection.fullUrl}{.end}{.end}{.end}"/>
    {.equal? item.recordTypeLabel "text"}{.if websiteSettings.ampEnabled}<link rel="amphtml" href="{website.baseUrl}{item.fullUrl}?format=amp">{.end}{.end}
    {.section collection}{.collectionTypeNameEquals? blog}<link rel="alternate" type="application/rss+xml" title="RSS Feed" href="{website.baseUrl}{fullUrl}?format=RSS"/>{.end}{.end}
    <meta property="og:site_name" content="{website.siteTitle}"/>
    <meta property="og:title" content="{.if item}{item.title} — {website.siteTitle}{.or}{.if collection}{collection.title} — {website.siteTitle}{.or}{website.siteTitle}{.end}{.end}"/>
    <meta property="og:url" content="{.if item}{website.baseUrl}{item.fullUrl}{.or}{.if collection}{.if collection.homepage}{website.baseUrl}/{.or}{website.baseUrl}{collection.fullUrl}{.end}{.end}{.end}"/>
    <meta property="og:type" content="website"/>
    <meta property="og:image" content="{.if item}{item.assetUrl}?format=1000w{.or}{.if collection}{.if collection.mainImage}{collection.mainImage.assetUrl}?format=1000w{.or}{website.logoImageUrl}?format=1000w{.end}{.end}{.end}"/>
    <meta property="og:description" content="{.if item}{item.excerpt|safe|htmlattr}{.or}{.if collection}{collection.description|safe|htmlattr}{.or}{website.siteDescription|safe|htmlattr}{.end}{.end}"/>
    <meta itemprop="name" content="{.if item}{item.title} — {website.siteTitle}{.or}{.if collection}{collection.title} — {website.siteTitle}{.or}{website.siteTitle}{.end}{.end}"/>
    <meta itemprop="url" content="{.if item}{website.baseUrl}{item.fullUrl}{.or}{.if collection}{.if collection.homepage}{website.baseUrl}/{.or}{website.baseUrl}{collection.fullUrl}{.end}{.end}{.end}"/>
    <meta itemprop="image" content="{.if item}{item.assetUrl}?format=1000w{.or}{.if collection}{.if collection.mainImage}{collection.mainImage.assetUrl}?format=1000w{.or}{website.logoImageUrl}?format=1000w{.end}{.end}{.end}">
    <meta itemprop="thumbnailUrl" content="{.if item}{item.assetUrl}?format=1000w{.or}{.if collection}{.if collection.mainImage}{collection.mainImage.assetUrl}?format=1000w{.or}{website.logoImageUrl}?format=1000w{.end}{.end}{.end}">
    <meta itemprop="description" content="{.if item}{item.excerpt|safe|htmlattr}{.or}{.if collection}{collection.description|safe|htmlattr}{.or}{website.siteDescription|safe|htmlattr}{.end}{.end}"/>
    <link rel="image_src" href="{.if item}{item.assetUrl}?format=1000w{.or}{.if collection}{.if collection.mainImage}{collection.mainImage.assetUrl}?format=1000w{.or}{website.logoImageUrl}?format=1000w{.end}{.end}{.end}">
    <meta name="twitter:title" content="{.if item}{item.title} — {website.siteTitle}{.or}{.if collection}{collection.title} — {website.siteTitle}{.or}{website.siteTitle}{.end}{.end}"/>
    <meta name="twitter:image" content="{.if item}{item.assetUrl}?format=1000w{.or}{.if collection}{.if collection.mainImage}{collection.mainImage.assetUrl}?format=1000w{.or}{website.logoImageUrl}?format=1000w{.end}{.end}{.end}">
    <meta name="twitter:url" content="{.if item}{website.baseUrl}{item.fullUrl}{.or}{.if collection}{.if collection.homepage}{website.baseUrl}/{.or}{website.baseUrl}{collection.fullUrl}{.end}{.end}{.end}"/>
    <meta name="twitter:card" content="summary"/>
    <meta name="twitter:description" content="{.if item}{item.excerpt|safe|htmlattr}{.or}{.if collection}{collection.description|safe|htmlattr}{.or}{website.siteDescription|safe|htmlattr}{.end}{.end}"/>
    <meta name="description" content="{.if item}{item.excerpt|safe|htmlattr}{.or}{.if collection}{collection.description|safe|htmlattr}{.or}{website.siteDescription|safe|htmlattr}{.end}{.end}"/>
    <script type="application/ld+json">{"@context":"http://schema.org","@type":"WebSite","url":"{website.baseUrl}","name":"{website.siteTitle}","description":"{website.siteDescription}","image":"{website.logoImageUrl}"}</script>
    <script type="application/ld+json">{"@context":"http://schema.org","@type":"Organization","legalName":"{websiteSettings.storeSettings.businessName}","address":"","email":"{websiteSettings.contactEmail}","telephone":"{websiteSettings.contactPhoneNumber}"}</script>
    <script type="application/ld+json">{"@context":"http://schema.org","@type":"LocalBusiness","address":"{websiteSettings.storeSettings.contactLocation.addressLine1}, {websiteSettings.storeSettings.contactLocation.addressLine2}, {websiteSettings.storeSettings.contactLocation.addressCountry}","image":"{website.logoImageUrl}","name":"{websiteSettings.storeSettings.businessName}"}</script>
    <script>
        SQUARESPACE_ROLLUPS = {};
        window.Static = {SQUARESPACE_CONTEXT:{websiteSettings: {websiteSettings|json}}};
        window.Template = {
            Util: {},
            Controllers: {}
        }
    </script>
    {.end}
</head>

<body class="page-{collection.typeLabel} {squarespace.page-classes}" id="{squarespace.page-id}">

<div id="navigator">
    {.section website}
    <header id="topBar" class="sqs-announcement-bar-dropzone">
        <div data-content-field="site-title" class="siteTitle">

            {.if logoImageUrl}
            <a href="/">
                <img id="logo" src="{logoImageUrl}?format=1500w" alt="{siteTitle}"/>
            </a>
            {.or}
            <h1>
                <a href="/">
                    <span id="site-title">{siteTitle}</span>
                </a>
            </h1>
            <script>
                Y.use('squarespace-ui-base', function (Y) {
                    Y.one(".siteTitle h1").plug(Y.Squarespace.TextShrink, {
                        triggerWidth: 640
                    });
                });
            </script>
            {.end}

            {.section siteTagLine}
            <h2 class="logo-subtitle">{@}</h2>
            {.end}
        </div>
        <div class="nav-container">
            <div id="mobileNav">
                <div class="wrapper">
                    <squarespace:navigation navigationId="pages" template="mobile-navigation"/>
                </div>
            </div>
            <squarespace:navigation navigationId="pages" template="navigation"/>
            <div class="site-player-block">
                <div class="site-player paused not-init">
                    <div class="site-player-wrapper">
                        <div id="fountainG">
                            <div class="fountainG fountainG_1"></div>
                            <div class="fountainG fountainG_2"></div>
                            <div class="fountainG fountainG_3"></div>
                            <div class="fountainG fountainG_4"></div>
                            <div class="fountainG fountainG_5"></div>
                        </div>
                        <div class="site-player-controls">
                            <a id="playButton">
                                <i class="icono-"></i>
                            </a>
                            <a id="volumeButton">
                                <i class="icono-volumeHigh"></i>
                            </a>
                            <input id="volControl" value="100" class="volume-control" type="range" min="0" max="100" step="1"></input>
                        </div>
                        <div class="track-name"><span>Now playing: </span></div>
                    </div>
                </div>
            </div>
            <div class="header-block">
                <squarespace:block-field id="headerBlock" class="header-block-content" columns="12" label="Header Content" annotation-alignment="bottom"/>
            </div>
        </div>
    </header>
    {.end}
    <div class="sqs-cart-dropzone"></div>
    <squarespace:query collection="cast" limit="2">
        {.if items}
        {.section items.0}
        <div class="castWrapper">
            <div id="castDiv" data-title="{title}" class="cast-container {.if customContent.showLive}show-live{.end} paused content-fit slide-into-view"
                 sqs-controller="Template.Controllers.CastController"
                 data-url="{oembed.url}"
                 data-facebook-url="{customContent.facebookUrl}"
                 data-soundcloud-url="{customContent.soundCloudUrl}"
                 data-shoutcast-url="{customContent.shoutcastUrl}">
                {.if customContent.gallery && customContent.showGallery}
                    <div class="stream-gallery">
                        <div class="swiper-container">
                            <div class="swiper-wrapper">
                                {.repeated section customContent.gallery}
                                    <div class="swiper-slide content-fit"><a class="content-fill" href="{.if clickthroughUrl}{clickthroughUrl}{.end}"><img {@|image-meta} data-load="false"/></a></div>
                                {.end}
                            </div>
                           <!-- <div class="swiper-pagination"></div>-->
                            <div class="swiper-button-prev arrow-butt"></div>
                            <div class="swiper-button-next arrow-butt"></div>
                        </div>
                    </div>
                {.or}
                    <img {@|image-meta} data-load="false"/>
                {.end}
                <div class="mobile-play-button">
                    <svg version="1.1" x="0px" y="0px"
                         viewBox="0 0 60 60" fill="#{colorData.centerAverage}">
                        <g>
                            <path d="M45.563,29.174l-22-15c-0.307-0.208-0.703-0.231-1.031-0.058C22.205,14.289,22,14.629,22,15v30
                            c0,0.371,0.205,0.711,0.533,0.884C22.679,45.962,22.84,46,23,46c0.197,0,0.394-0.059,0.563-0.174l22-15
                            C45.836,30.64,46,30.331,46,30S45.836,29.36,45.563,29.174z M24,43.107V16.893L43.225,30L24,43.107z"/>
                            <path d="M30,0C13.458,0,0,13.458,0,30s13.458,30,30,30s30-13.458,30-30S46.542,0,30,0z M30,58C14.561,58,2,45.439,2,30
                            S14.561,2,30,2s28,12.561,28,28S45.439,58,30,58z"/>
                        </g>
                    </svg>
                </div>
                <div class="live-indicator">● Live</div>
            </div>
        </div>
        {.end}
        {.end}
    </squarespace:query>
    <section id="container">
        <div class="content-loader">
            <div class="fountainG fountainG_1"></div>
            <div class="fountainG fountainG_2"></div>
            <div class="fountainG fountainG_3"></div>
            <div class="fountainG fountainG_4"></div>
            <div class="fountainG fountainG_5"></div>
        </div>
        <div id="container-content" data-content-field="main-content" role="main">
            {.section collection}{.equal? typeName "products"}{.section categories}
            <div id="categoryNav">
                <input type="checkbox" name="category-nav-toggle" id="categoryNavToggle"
                       class="category-nav-toggle hidden"/>
                <label for="categoryNavToggle" class="category-nav-toggle-label" onclick></label>
                <nav class="category-nav">
                    <ul class="category-nav-links">
                        <li class="filter"><a>Filter{.if categoryFilter}: {categoryFilter}{.end}</a></li>
                        <li
                        {.if categoryFilter}{.or} class="active-link all"{.end}><a href="{fullUrl}">All</a></li>
                        {.repeated section @}
                        <li
                        {.equal? categoryFilter @} class="active-link"{.end}><a
                            href="{fullUrl}?category={@|url-encode}">{@}</a></li>
                        {.end}
                    </ul>
                </nav>
            </div>
            {.end}{.end}{.end}
            <div class="controller" {.equal? collection.typeName "wall-index"}sqs-controller="Template.Controllers.WallController"{.end}>
                {squarespace.main-content}
            </div>
        </div>
    </section>
    {.section website}
    <footer id="bottomBar">
            <div class="footer-h-content"></div>
            <squarespace:block-field id="footerBlock" class="footer-block" columns="12" label="Footer Content"/>
    </footer>
    {.end}
</div>
<script>
    Y.use('squarespace-ui-base', function (Y) {
        Y.one(".project-item .meta h1") && Y.one(".project-item .meta h1").plug(Y.Squarespace.TextShrink);
    });
</script>
{.if authenticatedAccount}
<squarespace:script src="admin.js" combo="false"/>
<squarespace:script src="masonry.pkgd.min.js" combo="false"/>
<squarespace:script src="imagesloaded.pkgd.min.js" combo="false"/>
<squarespace:script src="moment.js" combo="true"/>
<squarespace:script src="moment-timezone.js" combo="true"/>
<squarespace:script src="jquery.min.js" combo="true"/>
<squarespace:script src="typed.min.js" combo="true"/>
<!--<squarespace:script src="json-template.js" combo="true"/>-->
<squarespace:script src="core-0.2.0.min.js" combo="true"/>
<squarespace:script src="sqs-controller.js" combo="true"/>
<squarespace:script src="ajaxLoader.js" combo="true"/>
<squarespace:script src="site.js" combo="true"/>
<squarespace:script src="wall-scripts/swiper.min.js" combo="true"/>
<squarespace:script src="wall-scripts/classie.js" combo="true"/>
<squarespace:script src="wall-scripts/AnimOnScroll.js" combo="true"/>
<squarespace:script src="share-buttons.js" combo="true"/>
<squarespace:script src="WallController.js" combo="true"/>
<squarespace:script src="soundcloud-api-widget.js" combo="true"/>
<squarespace:script src="mixcloud-api-widget.js" combo="true"/>
<squarespace:script src="ProjectController.js" combo="true"/>
<squarespace:script src="CastController.js" combo="false"/>
{squarespace-footers}
{.or}
<noscript id="loaderThings">
    <div id="header">{squarespace-headers}</div>
    <div id="footer">{squarespace-footers}</div>
    <div id="templateRevision">
        <squarespace:script src="t.js"/>
    </div>
</noscript>
{.end}
{.if template.mobileStylesEnabled}{.or}
<script>
    Y.use('node', function () {
        Y.on('domready', function () {
            var move;
            Y.all('.subnav a').each(function (a) {
                a.on('touchstart', function () {
                    move = false;
                });
                a.on('touchmove', function () {
                    move = true;
                });
                a.on('touchend', function () {
                    if (move === false) {
                        window.location = a.getAttribute('href');
                    }
                });
            });
        });
    });
</script>
{.end}
</body>
</html>
