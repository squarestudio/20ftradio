<html>
    <head>
        <meta name="viewport" content="viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
        {squarespace-headers}

    </head>
    <body class="cordova-app cordova-new-app page-{collection.typeLabel} {squarespace.page-classes}" id="{squarespace.page-id}">
    <style>
        html {
            -ms-touch-action: manipulation;
            touch-action: manipulation;
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        }

        body {
            min-height: 100vh;
            background: black;
            margin: 0;
            position: fixed;
        }

        .stream-found .loading-overlay {

        }

        .cordova-app.cordova-new-app #castDiv .mobile-image .line {
            position: absolute;
            display: block;
            width: 2.4%;
            height: 0;
            background-color: #fff;
            left: 69.4%;
            top: 80%;
            transform-origin: bottom center;
            -webkit-transform-origin: bottom center;
            transform: scaley(1);
            -webkit-transform: scaley(1);
        }

        .castWrapper {
            display: block !important;
            overflow: auto;
        }

        .cordova-app.cordova-new-app .stream-found .mobile-image .line {
            transform: scaley(600);
            -webkit-transform: scaley(600);
            height: 1px;
        }

        #castDiv .mobile-image, #container {
            will-change: transform;
        }

        #castDiv .mobile-image {
            overflow: visible !important;
        }

        #castDiv .mobile-image .pause, #castDiv .mobile-image .play {
            opacity: 0;
        }

        #castDiv .mobile-image .back {
            position: absolute;
            z-index: 10;
            opacity: 0;
        }

        #castDiv .mobile-image .back.gen {
            opacity: 1;
            bottom: 0;
            width: 82%;
            height: 68%;
        }

        .app-fullscreen .cordova-app .mobile-image .line {
            display: block !important;
            z-index: 5;
        }

        .stream-found #castDiv .mobile-image .line {
            transform: scaley(600) !important;
            -webkit-transform: scaley(600) !important;
            height: 1px !important;
        }

        .stream-found #castDiv .mobile-image .play {
            opacity: 1;
        }

        .stream-found #castDiv.playing .mobile-image .play {
            opacity: 0;
        }

        .stream-found #castDiv.playing .mobile-image .pause {
            opacity: 1;
        }

        .cordova-app #navigator {
            overflow: hidden;
            opacity: 1;
        }
    </style>
    <div id="navigator">
        {.section website}
        <header id="topBar" class="sqs-announcement-bar-dropzone">
            <div class="nav-container">
                <div id="mobileNav">
                    <div class="wrapper">
                        <squarespace:navigation navigationId="app-links" template="mobile-navigation"/>
                    </div>
                </div>
                <squarespace:navigation navigationId="pages" template="navigation"/>
                <div class="header-block">
                    <squarespace:block-field id="headerBlock" class="header-block-content" columns="12" label="Header Content" annotation-alignment="bottom"/>
                </div>
                <div class="site-player-block">
                    <div class="site-player paused not-init">
                        <div class="site-player-wrapper">
                            <div class="track-name"><span>Now playing: </span></div>
                            <div class="site-player-controls">
                                <a id="videoButton">
                                    <i class="icono-"></i>
                                </a>
                                <a id="playButton">
                                    <i class="icono-"></i>
                                </a>
                                <a id="volumeButton">
                                    <i class="icono-volumeHigh"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <canvas id="visualCanvas" class="v-align"></canvas>
                </div>
            </div>
        </header>
        {.end}
        <div class="sqs-cart-dropzone"></div>
        <squarespace:query collection="cast" limit="2">
            {.if items}
            {.section items.0}
            <div class="castWrapper">
                <div id="castDiv" data-title="{title}" class="cast-container {.if customContent.showLive}show-live{.end} paused content-fit"
                     sqs-controller="Template.Controllers.MobileCastController"
                     data-url="{oembed.url}"
                     data-facebook-url="{customContent.facebookUrl}"
                     data-soundcloud-url="{customContent.soundCloudUrl}"
                     data-shoutcast-url="{customContent.shoutcastUrl}">
                    <div class="mobile-image">
                        <svg class="back gen h-align" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1019.73 956"><defs><style>.cls-1{fill:#fff;}.cls-2{fill:#00a89c;}.cls-3{fill:#c1272d;}.cls-4{fill:#29a9e1;}</style></defs><g><g><path class="cls-1" d="M254.93,127.47A127.47,127.47,0,0,1,382.4,254.93H509.87C509.87,114.14,395.73,0,254.93,0S0,114.14,0,254.93H127.47A127.47,127.47,0,0,1,254.93,127.47Z"/><path class="cls-2" d="M509.87,254.93H382.4A127.47,127.47,0,0,1,254.93,382.4V509.87C395.73,509.87,509.87,395.73,509.87,254.93Z"/><path class="cls-3" d="M0,637.33H127.47A127.47,127.47,0,0,1,254.93,509.87V382.4C114.14,382.4,0,496.54,0,637.33Z"/><rect class="cls-4" y="637.33" width="127.47" height="127.47"/><path class="cls-1" d="M517.9,637.33H127.47V764.8H517.9a257,257,0,0,1,0-127.47Z"/><path class="cls-3" d="M777.55,446.45q-6.34-.31-12.75-.32c-4.27,0-8.52.12-12.75.33C638.92,452,545.18,531.34,517.9,637.33a257,257,0,0,0,0,127.47C546.2,874.75,646,956,764.8,956c140.8,0,254.93-114.14,254.93-254.93C1019.73,564.55,912.42,453.09,777.55,446.45ZM764.8,828.53A127.47,127.47,0,1,1,892.27,701.07,127.47,127.47,0,0,1,764.8,828.53Z"/><polygon class="cls-1" points="994.24 254.93 841.28 254.93 815.79 254.93 815.79 280.43 815.79 305.92 815.79 331.41 815.79 382.4 841.28 382.4 841.28 331.41 892.27 331.41 892.27 305.92 841.28 305.92 841.28 280.43 930.51 280.43 930.51 382.4 956 382.4 956 280.43 994.24 280.43 994.24 254.93"/><circle fill="#000" cx="764.08" cy="702" r="129"/></g></g><g class="mobile-trigger">
                            <g class="play" fill="#fff">
                                <polygon points="720.5,606.3 720.5,797.7 870.9,702"/>
                            </g>
                            <g class="pause" fill="#fff">
                                <path d="M689,789.6h50.1V614.4H689V789.6z M789.1,614.4v175.2h50.1V614.4H789.1z"/>
                            </g>
                        </g></svg>
                        <div class="line"></div>
                    </div>
                    <div class="live-indicator">‚óè Live</div>
                </div>
            </div>
            {.end}
            {.end}
        </squarespace:query>
        <section id="container">
            <div class="content-loader">
                <div class="fountainG fountainG_1"></div>
                <div class="fountainG fountainG_2"></div>
                <div class="fountainG fountainG_3"></div>
                <div class="fountainG fountainG_4"></div>
                <div class="fountainG fountainG_5"></div>
            </div>
            <div id="container-content" class="clear" data-content-field="main-content" role="main">
                <div id="controller" class="controller" {.equal? collection.typeName "wall-index"}sqs-controller="Template.Controllers.WallController"{.or}{.equal? collection.urlId "mobile-app"}sqs-controller="Template.Controllers.MobileEventsController"{.end}{.end}>
                    {.equal? collection.urlId "mobile-app"}
                    <div id="mobileEvents">
                        <div class="tabs clear">
                            <a href="#mobile-events-upcoming" class="tab-1 active">Schedule</a>
                            <a href="#mobile-events-past" class="tab-2">Shows</a>
                            <div class="tab-border"></div>
                        </div>
                        <div class="content-loader" style="display:block;">
                            <div class="fountainG fountainG_1"></div>
                            <div class="fountainG fountainG_2"></div>
                            <div class="fountainG fountainG_3"></div>
                            <div class="fountainG fountainG_4"></div>
                            <div class="fountainG fountainG_5"></div>
                        </div>
                        <div class="mobileEvents-wrapper">
                            <div class="mobileEvents-container">
                                <ul id="mobile-events-upcoming" class="mobileEvents mobileEvents-Upcoming active" style="opacity: 0">
                                    <p class="no-shows-message">No Upcoming Shows</p>
                                </ul>
                                <ul id="mobile-events-past" class="mobileEvents mobileEvents-Past"></ul>
                            </div>
                        </div>
                    </div>
                    {.or}
                    {squarespace.main-content}
                    {.end}
            </div>
    </section>
    </div>
    <footer id="bottomBar">
        <div id="mixcloudInsert"></div>
        <div class="new-footer-content">
            <a href="#" class="menu-button footer-button">Menu</a>
            <a href="#" class="mode-button footer-button">Mode</a>
            <a href="#" class="follow-button footer-button">Follow</a>
        </div>
        <div class="footer-h-content">
            <!--<squarespace:block-field id="mobileFooterBlock" class="footer-block" columns="12" label="Mobile Footer Content"/>-->
        </div>
    </footer>
    <audio id="shoutcastPlayer" muted src="http://eu2.radioboss.fm:8188/20ftRadio" autoplay style="display: none" class="stream-player" poster="https://www.20ftradio.net/assets/icon.png" crossorigin="anonymous" preload="auto" playsinline="" -webkit-playsinline="" name="media"></audio>
    <script>
        window.Template = {
            Util: {},
            Controllers: {}
        }
    </script>
    <squarespace:script src="masonry.pkgd.min.js" combo="false"/>
    <squarespace:script src="imagesloaded.pkgd.min.js" combo="false"/>
    <squarespace:script src="moment.js" combo="true"/>
    <squarespace:script src="moment-timezone.js" combo="true"/>
    <squarespace:script src="jquery.min.js" combo="true"/>
    <squarespace:script src="typed.min.js" combo="true"/>
    <!--<squarespace:script src="json-template.js" combo="true"/>-->
    <squarespace:script src="core-0.2.0.min.js" combo="true"/>
    <squarespace:script src="sqs-controller.js" combo="true"/>
    <squarespace:script src="ajaxLoader.js" combo="true"/>
    <squarespace:script src="app-site.js" combo="true"/>
    <squarespace:script src="wall-scripts/swiper.min.js" combo="true"/>
    <squarespace:script src="wall-scripts/classie.js" combo="true"/>
    <squarespace:script src="wall-scripts/AnimOnScroll.js" combo="true"/>
    <squarespace:script src="share-buttons.js" combo="true"/>
    <squarespace:script src="WallController.js" combo="true"/>
    <squarespace:script src="soundcloud-api-widget.js" combo="true"/>
    <squarespace:script src="mixcloud-api-widget.js" combo="true"/>
    <squarespace:script src="ProjectController.js" combo="true"/>
    <squarespace:script src="sha1.js" combo="true"/>
    <squarespace:script src="payment.js" combo="true"/>
    <squarespace:script src="MobileCastController.js" combo="true"/>
    <squarespace:script src="MobileEventsController.js" combo="true"/>
    <squarespace:script src="general.js" combo="true"/>
    <squarespace:script src="mobile-app.js" combo="true"/>
    {squarespace-footers}
    <script>
        function initFirstInteract() {
            var video = document.getElementById('shoutcastPlayer');
            var navigator = document.querySelector('html');
            var mobileTrigger = document.querySelector('.mobile-trigger');
            mobileTrigger.addEventListener('click', function (e) {
                e.preventDefault();
                console.log(window.CASTHERE);
                if (!window.CASTHERE) {
                    if (!window.firstPlayClick) {
                        video.pause();
                        window.firstPlayClick = true;
                        setTimeout(function () {
                            video.volume = 1;
                            video.muted = false;
                            video.autoplay = false;
                            console.log('clicked');
                            video.play();
                        }, 16)
                    } else {
                        if (video.paused) {
                            video.play();
                            video.muted = false;
                        } else {
                            video.pause();
                        }
                    }
                }
            }, false);
        }
        initFirstInteract();
        var audio = document.getElementById('shoutcastPlayer');
        audio.volume = 0;
        audio.muted = true;
        audio.play();
        if (navigator.platform.indexOf('iP')>-1){
            //audio.pause();
        }
        setTimeout(function () {
            if (!window.CASTHERE&&document.getElementById('castDiv').className.indexOf('playing')===-1) {
                audio.pause();
            }
        }, 20000);
        audio.addEventListener('playing', function () {
            if (!window.CASTHERE) {
                console.log('playING', window.firstPlayClick);
                if(window.firstPlayClick){
                    var navigator = document.querySelector('html');
                    navigator.className = ' stream-found';
                    document.getElementById('castDiv').className += ' playing';
                }
            }
        });
        audio.addEventListener('pause', function () {
            if (!window.CASTHERE) {
                console.log('pauSED');
                document.getElementById('castDiv').className =  document.getElementById('castDiv').className.replace(' playing', '');
            }
        });
        audio.addEventListener('canplaythrough', function () {
            if (!window.CASTHERE) {
                console.log('CANPLAY');
                var navigator = document.querySelector('html');
                navigator.className = navigator.className.replace(' stream-found','')+' stream-found';
            }
        });
        var motionPlayActive = false;
        window.ondevicemotion = function(){
            if(motionPlayActive) return;
            console.log('Motion Play Activate');
            motionPlayActive = true;
            audio.play();
        }
        var $body = $('body');
        var $html = $('html');
        $html.addClass('platform-' + window.navigator.platform);
        $html.addClass('model-' + window.navigator.model);
        if (navigator.platform === 'iPhone' && window.innerHeight === 812) {
            $html.addClass('device-IphoneX');
        }
        $body.delegate('.follow-button', 'click', function (e) {
            e.preventDefault();
            $body.toggleClass('mobile-share-active');
        });
        $body.delegate('.mode-button', 'click', function (e) {
            e.preventDefault();
            $html.toggleClass('full-mode-active');
            if (!$html.hasClass('full-mode-active')) {
                window.scrollTo(0, 0);
            }
        });
        $body.delegate('.menu-button', 'click', function (e) {
            e.preventDefault();
            $body.toggleClass('mobile-menu-active');
        });
        setTimeout(function () {
            Y.fire('appready');
        }, 100);
    </script>
    </body>
</html>
