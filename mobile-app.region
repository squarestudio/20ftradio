<html>

<head>
    <meta name="viewport" content="viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    {squarespace-headers}
    <noscript id="loaderThings">
        <squarespace:script src="MobileCastController.js" combo="true" />
        <squarespace:script src="MobileEventsController.js" combo="true" />
        <squarespace:script src="general.js" combo="true" />
        <squarespace:script src="mobile-app.js" combo="true" />
    </noscript>
</head>

<body class="cordova-app cordova-new-app page-{collection.typeLabel} {squarespace.page-classes}" id="{squarespace.page-id}">
            <style>html {
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

body {
    min-height: 100vh;
    background: black;
    margin: 0;
    position: fixed;
}

.stream-found .loading-overlay {

}

#castDiv:after {
    content: 'Internet Connection Requred';
    text-transform: uppercase;
    font-size: 1em;
    color: #fff;
    display: none;
    text-align: center;
    position: absolute;
    bottom: -10%;
    z-index: 2000;
    width: 100%;
    left: 0;
}

.connection-offline #castDiv:after {
    display: block;
}

.connection-offline .cordova-new-app #castDiv .mobile-image {
    transform: translateY(0) !important;
}

.connection-offline .cordova-new-app #container {
    transform: translateY(110%) !important;
}

.connection-offline #bottomBar {
    transform: translateY(150%);
}

.cordova-app #bottomBar {
    transition: transform .2s ease;
}

.screenAspect-1d33 .cordova-app.cordova-new-app #castDiv .mobile-image .line {
    left: 66%;
}

.content-loader {
    background-image: url("../img/loader.svg");
    background-position: center;
    background-repeat: no-repeat;
    background-size: contain;
    height: 42px;
}

.cordova-app.cordova-new-app #castDiv .mobile-image .line {
    position: absolute;
    display: block;
    width: 2.4%;
    max-width: 12px;
    height: 0;
    background-color: #fff;
    left: 69.4%;
    top: 80%;
    transform-origin: bottom center;
    -webkit-transform-origin: bottom center;
    transform: scaley(1);
    -webkit-transform: scaley(1);
}

.castWrapper {
    display: block !important;
    overflow: auto;
}

.cordova-app.cordova-new-app .stream-found .mobile-image .line {
    transform: scaley(600);
    -webkit-transform: scaley(600);
    height: 1px;
}

#castDiv .mobile-image, #container {
    will-change: transform;
}

.back-circle {
    stroke: rgba(255, 255, 255, 1);
    stroke-width: 8;
    stroke-dasharray: 1000;
    stroke-dashoffset: 1000;
}

@keyframes dash {
    to {
        stroke-dashoffset: 0;
    }
}

@keyframes dashQuick {
    80% {
        stroke-dashoffset: 0;
    }
    100% {
        stroke-dashoffset: 1000;
        stroke: rgba(255, 255, 255, 0);
    }
}
.full-mode-active .back-circle, .device-ready .paused .back-circle {
    animation: none!important;
    stroke-dashoffset: 1000;
}
.device-ready.init-play.data-playing .back-circle {
    animation: dashQuick .8s ease-out forwards;
}

.device-ready.init-play.data-loading .playing .back-circle {
    animation: dash 24s linear infinite;
}

.device-ready #castDiv .mobile-image .play {
    opacity: 1;
}

.device-ready #castDiv .mobile-image .play {
    opacity: 1;
}

#castDiv .mobile-image .pause, #castDiv .mobile-image .play {
    opacity: 0;
}

#castDiv .mobile-image {
    overflow: visible !important;
}

#castDiv .mobile-image .back {
    position: absolute;
    z-index: 10;
    opacity: 0;
}

#castDiv .mobile-image .back.gen {
    opacity: 1;
    bottom: 0;
    width: 82%;
    height: 68%;
}

.app-fullscreen .cordova-app .mobile-image .line {
    display: block !important;
    z-index: 5;
}

.stream-found #castDiv .mobile-image .line {
    transform: scaley(600) !important;
    -webkit-transform: scaley(600) !important;
    height: 1px !important;
}

.stream-found #castDiv .mobile-image .play {
    opacity: 1;
}

.init-play #castDiv.playing .mobile-image .play {
    opacity: 0;
}

.init-play #castDiv.playing .mobile-image .pause {
    opacity: 1;
}

.cordova-app #navigator {
    overflow: hidden;
    opacity: 1;
}
